
<DashboardItem Title="Results" Description="Below is a summary of all of the suspicious aspects of the email you uploaded">
    <ComponentBody>
        <MudButton OnClick="@GenerateResults" Color="Color.Primary" Variant="Variant.Filled" FullWidth>Generate Results</MudButton>
        @foreach (string warning in warnings)
        {
            <MudAlert Severity="Severity.Warning">@warning</MudAlert>
        }


    </ComponentBody>
    <ComponentHelp>
        <ResultsHelp/>
    </ComponentHelp>

</DashboardItem>

